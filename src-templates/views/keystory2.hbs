<section class="keystory-main">
  <div class="keystory-main-text">
    <h1 class="title">Keystory</h1>
    <h2 class="sub">Welcome to the Family</h2>
  </div>
  <div class="scroll-down-container">
    <div class="scroll-down-button" data-controller="Keystory" data-action="initScrollDownButton">
      <div></div>
    </div>
  </div>
</section>

<section class="keystory-testimonials">
  <ul class="quote-list">
    <li class="quote tag-line">
      <div class="tag-container">
        <h1>
        </h1>
        <div class="underline"></div>
      </div>
    </li>
    {{# each data.posts}}
    <li class="quote">
      <div class="quotes">
        <span class="open-quote"></span>
        <span class="close-quote"></span>
      </div>
      <div class="content">{{{content}}} </div>
      <div class="credit">- {{{author}}} </div>
    </li>

    {{/each}}
  </ul>
</section>



	</head>
	<body>
		<!-- header start !-->
		<nav id="header">
			<a href="/"><div class="nav-logo"></div></a>
			<ul class="nav-buttons">
				<li class="nav-button"><a href="/about">About</a></li>
				<li class="nav-button"><a href="/service">Service</a></li>
				<li class="nav-button"><a href="/programs">Programs</a></li>
				<li class="nav-button"><a href="/keystory">Keystory</a></li>
				<li class="nav-button"><a href="/career">Career</a></li>
				<li class="nav-button"><a href="/contact">Contact</a></li>
			</ul>


			<a class="mob-nav-toggle-button" data-controller="Nav" data-action="initMobileNav"><span></span></a>
			<div class="mob-nav-buttons-container">
				<ul class="mob-nav-buttons">
					<a href="/about"><li class="mob-nav-button">About</li></a>
					<a href="/service"><li class="mob-nav-button">Service</li></a>
					<a href="/programs"><li class="mob-nav-button">Programs</li></a>
					<a href="/keystory"><li class="mob-nav-button">Keystory</li></a>
					<a href="/career"><li class="mob-nav-button">Career</li></a>
					<a href="/contact"><li class="mob-nav-button">Contact</li></a>
				</ul>
			</div>

		</nav>
		<!-- header end !-->

		<!--
			Flash messages allow you to display once-off status messages to users, e.g. form
			validation errors, success messages, etc
		-->
		{{#if messages}}
		<div id="flash-messages" class="container">
			{{#if messages.info}}
			<div class="alert alert-info">
					{{{flashMessages messages.info}}}
			</div>
			{{/if}}
			{{#if messages.success}}
			<div class="alert alert-success">
					{{{flashMessages messages.success}}}
			</div>
			{{/if}}
			{{#if messages.warning}}
			<div class="alert alert-warning">
					{{{flashMessages messages.warning}}}
			</div>
			{{/if}}
			{{#if messages.error}}
			<div class="alert alert-danger">
					{{{flashMessages messages.error}}}
			</div>
			{{/if}}
		</div>
		{{/if}}


		<div class="alert-message">
      <div class="alert alert-success" role="alert">
      	<h1>Thank You!</h1>
      	<div class="underline"></div>
        <span>Thanks for your message! We'll reply you shortly.</span>
      </div>
      <div class="alert alert-info" role="alert">
        You have already subscribed!
      </div>
      <div class="alert alert-danger" role="alert">
      	<h1>Sorry!</h1>
      	<div class="underline"></div>
        <span>
        	Oops! There seems to be an error! Please try again.
        </span>
      </div>
    </div>
		<!-- The content block should contain the body of your template's content-->
		{{{body}}}




		<div id="footer">
			<a href="/">

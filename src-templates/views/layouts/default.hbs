sform:translateZ(0)}@media (max-width:900px){.nav-buttons{display:none}}nav .active{color:#9a0107}nav li a{text-decoration:none!important}.nav-button{text-transform:uppercase;line-height:70px;display:table-cell;padding:4px 14px 0}.mob-nav-toggle-button{position:absolute;right:0;height:70px;width:70px;line-height:70px;cursor:pointer;padding:32.5px 10px 0}@media (min-width:901px){.mob-nav-toggle-button{display:none}}.mob-nav-toggle-button span,.mob-nav-toggle-button span:after,.mob-nav-toggle-button span:before{cursor:pointer;border-radius:1px;height:5px;width:35px;background:#b72025;position:absolute;display:block;content:'';-webkit-transition:all .5s ease;transition:all .5s ease}.mob-nav-toggle-button span:before{top:-10px}.mob-nav-toggle-button span:after{bottom:-10px}.mob-nav-toggle-button.active span{background-color:transparent}.mob-nav-toggle-button.active span:after,.mob-nav-toggle-button.active span:before{top:0}.mob-nav-toggle-button.active span:before{-webkit-transform:rotate(45deg);transform:rotate(45deg)}.mob-nav-toggle-button.active span:after{-webkit-transform:translateY(-10px) rotate(-45deg);transform:translateY(-10px) rotate(-45deg);top:10px}.mob-nav-buttons-container{-webkit-transition:height .5s ease;transition:height .5s ease;z-index:1;position:absolute;top:70px;width:100%;background:#fff;height:0;overflow:hidden}.mob-nav-buttons{list-style:none;padding:0;margin:0;text-align:right;font-size:18px}.mob-nav-buttons a,.mob-nav-buttons li{text-decoration:none!important}.mob-nav-button{padding:10px 20px}#footer{background:#323232;padding:55px 0 60px}.footer-logo{background-image:url(../images/footer-logo.png);background-position:center center;background-repeat:no-repeat;background-size:contain;width:160px;height:80px;margin-left:auto;margin-right:auto}.footer-line-separator{height:.15px;background:#fff;opacity:.5;height:1px;width:70%;max-width:800px;margin:30px auto}.footer-content{font-size:12px;margin:0 auto;width:65%;max-width:730px;text-align:center}.footer-content .copyright{float:right;color:#fff}.footer-content .contact,.footer-content .privacy{color:#fff;margin:0 7px;float:left}.footer-content .sns{float:right;padding-top:5px}.footer-content .sns .icon{display:inline-block;width:25px;height:25px;margin:0 4px}.footer-content .sns .twitter .icon{background-image:url(../images/footer-twitter.png)}.footer-content .sns .linkedin .icon,.footer-content .sns .twitter .icon{background-position:center center;background-repeat:no-repeat;background-size:contain}.footer-content .sns .linkedin .icon{background-image:url(../images/footer-linkedin.png)}.footer-content .sns .facebook .icon{background-image:url(../images/footer-facebook.png);background-position:center center;background-repeat:no-repeat;background-size:contain}

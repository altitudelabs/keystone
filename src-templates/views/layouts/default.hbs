		html{height:100%;margin:0}*{-webkit-font-smoothing:antialiased;font-family:HelveticaNeueLTStd}body{min-height:100%;height:1px;margin:0;padding-top:70px;width:100%}a,a:hover{color:#575656}.contact-form{background:#b72025;text-align:center;padding-top:70px;padding-bottom:80px}.contact-form .title-small{color:#fff;font-size:20px;font-weight:700}.contact-form .underline{width:40px;height:2px;background:#fff;margin:auto;margin-bottom:20px}.contact-form .title{font-size:40px;color:#fff;font-weight:700}.contact-form form{padding-top:50px}.contact-form .form-group{width:70%;max-width:500px;margin-left:auto;margin-right:auto;font-weight:700;padding-bottom:20px}.contact-form .form-group input,.contact-form .form-group textarea{color:#fff;border-radius:0;border:0;font-size:18px;background-color:#9a0107!important}.contact-form .form-group input{height:60px;line-height:60px}.contact-form .form-group ::-webkit-input-placeholder{color:#fff}.contact-form .form-group :-moz-placeholder,.contact-form .form-group ::-moz-placeholder{color:#fff}.contact-form .form-group :-ms-input-placeholder{color:#fff}.contact-form button.send{width:180px;height:50px;line-height:50px;font-weight:700;font-size:18px;color:#fff;border-radius:10px;background:transparent;border:2px solid #fff}#mce-responses .response{color:#fff;font-size:18px;width:70%;margin:auto;top:20%;text-align:center}.alert-message{text-align:center;width:100%;position:fixed;left:0;top:0;height:100%;z-index:2;background-color:rgba(0,0,0,.4);display:none}.alert-message .alert-danger,.alert-message .alert-info,.alert-message .alert-success{position:absolute;width:25%;max-width:450px;min-width:250px;top:40%;left:0;right:0;border:1px solid;border-radius:0;margin-left:auto;margin-right:auto;text-align:middle;display:none;background-color:#fff;border-radius:8px;border:0;padding:30px 0 50px}.alert-message .alert-danger h1,.alert-message .alert-info h1,.alert-message .alert-success h1{margin-top:20px;color:#b72025;font-weight:700}.alert-mes